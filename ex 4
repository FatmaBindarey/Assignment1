#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	float x;
	float y;
printf( " enter two number :");
fflush(stdin); fflush(stdout);
scanf(" %f %f",&x,&y);
printf(" multiply is %f ",x*y);

}
