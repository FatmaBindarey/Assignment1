#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int x;
	int y;
printf( " enter two number :");
fflush(stdin); fflush(stdout);
scanf(" %d %d",&x,&y);
printf(" sum is %d ",x+y);

}

